{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@Layouts": ["src/layouts"],
      "@Layouts/*": ["src/layouts/*"],
      "@Components": ["src/components"],
      "@Components/*": ["src/components/*"],
      "@Contexts": ["src/contexts"],
      "@Contexts/*": ["src/contexts/*"],
      "@Utils": ["src/utils"],
      "@Utils/*": ["src/utils/*"]
    }
  },
  "include": [
    "./src/**/*"
  ]
}
